package com.usp.util;

public class ByteArrayEquivalent {

	private byte[] bytes;
	private double[][] array;
	private int rows;
	private int cols;
	private double max;
	private double min;

	public ByteArrayEquivalent(double[][] array) {
		
	}

	public int getRows() {
		return rows;
	}

	public int getCols() {
		return cols;
	}

	public double getMax() {
		return max;
	}

	public double getMin() {
		return min;
	}

	public double[][] getArray() {
		return array;
	}
}
